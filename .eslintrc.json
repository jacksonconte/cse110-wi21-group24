{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "curly": ["error", "all"],
        "semi": ["error", "always"]
    },
    "noInlineConfig": true
}
